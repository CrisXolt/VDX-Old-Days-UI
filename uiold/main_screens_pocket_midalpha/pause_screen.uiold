                    }
                  }
                ]
              }
            },
            {
              "padding_score_3": {
                "type": "panel",
                "size": [ "100%c", 8 ],
                "controls": [
                  { 
                    "score@scoreboard.player_score": {
                      "size": [ "default", 10 ],
                      "color": "$player_score_color",
                      "anchor_to": "top_right",
                      "anchor_from": "top_right",
                      "offset": [ 0, 4 ],
                      "shadow": true
                    }
