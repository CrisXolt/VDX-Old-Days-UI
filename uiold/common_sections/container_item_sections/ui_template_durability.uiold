      { "durability_bar_07@common_durability_old.durability_bar_07": { "$min_amount": 0.4998, "$max_amount": 0.5712 } },
      { "durability_bar_08@common_durability_old.durability_bar_08": { "$min_amount": 0.5712, "$max_amount": 0.6426 } },
      { "durability_bar_09@common_durability_old.durability_bar_09": { "$min_amount": 0.6426, "$max_amount": 0.7140 } },
      { "durability_bar_10@common_durability_old.durability_bar_10": { "$min_amount": 0.7140, "$max_amount": 0.7854 } },
      { "durability_bar_11@common_durability_old.durability_bar_11": { "$min_amount": 0.7854, "$max_amount": 0.8568 } },
      { "durability_bar_12@common_durability_old.durability_bar_12": { "$min_amount": 0.8568, "$max_amount": 0.9282 } },
      { "durability_bar_13@common_durability_old.durability_bar_13": { "$min_amount": 0.9282, "$max_amount": 1.0000 } }
    ]
  }
}